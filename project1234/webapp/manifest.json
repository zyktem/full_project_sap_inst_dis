{
  "_version": "1.48.0",
  "sap.app": {
    "id": "project1234",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.4",
      "toolsId": "b13870ea-75fe-433c-8ee9-85adc6f619d3"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1234.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "localModel":{
        "type": "sap.ui.model.json.JSONModel"
        
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "project1234.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name": "RouteView2",
          "pattern": "RouteView2",
          "target": [
            "TargetView2"
          ]
        },
        {
          "name": "RouteView3",
          "pattern": "RouteView3",
          "target": [
            "TargetView3"
          ]
        },
        {
          "name": "serdis",
          "pattern": "dismantle",
          "target": [
            "serdis"
          ]
        },
        {
          "name": "nsirdis",
          "pattern": "dismantle",
          "target": [
            "nssirdis"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "viewType": "XML",
          "transition": "show",
          "clearControlAggregation": false,
          "viewId": "View1",
          "viewName": "View1"
        },
        
        "TargetView2": {
          "viewType": "XML",
          "transition": "show",
          "clearControlAggregation": false,
          "viewId": "View2",
          "viewName": "View2"
        },
        "TargetView3": {
          "viewType": "XML",
          "transition": "show",
          "clearControlAggregation": false,
          "viewId": "View3",
          "viewName": "View3"
        },
        "serdis": {
          "viewType": "XML",
          "transition": "show",
          "clearControlAggregation": false,
          "viewId": "serdis",
          "viewName": "serdis"
        },
        "nsirdis": {
          "viewType": "XML",
          "transition": "show",
          "clearControlAggregation": false,
          "viewId": "nsirdis",
          "viewName": "nsirdis"
        }
      }
    },
    "rootView": {
      "viewName": "project1234.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
